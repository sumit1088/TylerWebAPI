<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:securecase="urn:tyler:ecf:v5.0:extensions:securecase" xmlns:ecf="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/" xmlns:structures="http://release.niem.gov/niem/structures/4.0/" xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:tyler:ecf:v5.0:extensions:securecase" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="..\ecf.xsd" namespace="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" />
  <xs:import schemaLocation="..\niem\niem-core\4.0\niem-core.xsd" namespace="http://release.niem.gov/niem/niem-core/4.0/" />
  <xs:import schemaLocation="..\niem\proxy\xsd\4.0\xs.xsd" namespace="http://release.niem.gov/niem/proxy/xsd/4.0/" />
  <xs:import schemaLocation="..\niem\utility\structures\4.0\structures.xsd" namespace="http://release.niem.gov/niem/structures/4.0/" />
  <xs:complexType name="SecureCaseMessageType">
    <xs:annotation>
      <xs:documentation>Message to secure a case.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ecf:RequestMessageType">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" ref="ecf:CaseTrackingID" />
          <xs:element minOccurs="1" maxOccurs="1" ref="securecase:IsSecure" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="securecase:SecureCaseMessageAugmentationPoint" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="SecureCaseMessage" type="securecase:SecureCaseMessageType">
    <xs:annotation>
      <xs:documentation>Message to secure a case.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="IsSecure" type="niem-xs:boolean" />
  <xs:element abstract="true" name="SecureCaseMessageAugmentationPoint">
    <xs:annotation>
      <xs:documentation>An extension point for the enclosing message.</xs:documentation>
    </xs:annotation>
  </xs:element>
</xs:schema>