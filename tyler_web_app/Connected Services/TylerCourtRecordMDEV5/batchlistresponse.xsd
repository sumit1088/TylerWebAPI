<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:ecf="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/" xmlns:batchlistresponse="urn:tyler:ecf:v5.0:extensions:batchlistresponse" xmlns:structures="http://release.niem.gov/niem/structures/4.0/" xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/" xmlns:common="urn:tyler:ecf:v5.0:extensions:common" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:tyler:ecf:v5.0:extensions:batchlistresponse" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="../ecf.xsd" namespace="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" />
  <xs:import schemaLocation="../niem/niem-core/4.0/niem-core.xsd" namespace="http://release.niem.gov/niem/niem-core/4.0/" />
  <xs:import schemaLocation="../niem/proxy/xsd/4.0/xs.xsd" namespace="http://release.niem.gov/niem/proxy/xsd/4.0/" />
  <xs:import schemaLocation="../niem/utility/structures/4.0/structures.xsd" namespace="http://release.niem.gov/niem/structures/4.0/" />
  <xs:import schemaLocation="common.xsd" namespace="urn:tyler:ecf:v5.0:extensions:common" />
  <xs:complexType name="BatchType">
    <xs:complexContent mixed="false">
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" ref="common:BatchIdentification" />
          <xs:element minOccurs="1" maxOccurs="1" ref="common:BatchStatus" />
          <xs:element minOccurs="0" maxOccurs="1" ref="common:HearingSchedule" />
          <xs:element minOccurs="1" maxOccurs="1" ref="nc:DocumentFiledDate" />
          <xs:element minOccurs="1" maxOccurs="unbounded" ref="common:Envelope" />
          <xs:element minOccurs="0" maxOccurs="1" ref="batchlistresponse:BatchSize" />
          <xs:element minOccurs="0" maxOccurs="1" ref="batchlistresponse:TotalFees" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GetBatchListResponseMessageType">
    <xs:annotation>
      <xs:documentation>Message to return batches.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ecf:ResponseMessageType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="batchlistresponse:Batch" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="batchlistresponse:GetBatchListResponseMessageAugmentationPoint" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="GetBatchListResponseMessage" type="batchlistresponse:GetBatchListResponseMessageType">
    <xs:annotation>
      <xs:documentation>Message to retrieve batches.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element abstract="true" name="GetBatchListResponseMessageAugmentationPoint">
    <xs:annotation>
      <xs:documentation>An extension point for the enclosing message.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Batch" type="batchlistresponse:BatchType" />
  <xs:element name="TotalFees" type="nc:AmountType" />
  <xs:element name="BatchSize" type="nc:NonNegativeDecimalType" />
</xs:schema>