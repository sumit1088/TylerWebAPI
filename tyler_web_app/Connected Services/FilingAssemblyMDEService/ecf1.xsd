  <xs:import schemaLocation="niem\utility\structures\4.0\structures.xsd" namespace="http://release.niem.gov/niem/structures/4.0/" />
  <xs:annotation>
    <xs:documentation> Schema for namespace https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf</xs:documentation>
  </xs:annotation>
  <xs:complexType name="CallbackMessageType">
    <xs:annotation>
      <xs:documentation>The base message for an asynchronous response to a message.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ecf:CaseFilingType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:ConnectedDocumentReview" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:FilingCompletionDate" />
          <xs:element minOccurs="1" maxOccurs="1" ref="ecf:FilingStatus" />
          <xs:element minOccurs="1" maxOccurs="unbounded" ref="ecf:LeadDocumentReview" />
          <xs:element minOccurs="1" maxOccurs="1" ref="nc:Case" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CaseAugmentationType">
    <xs:complexContent mixed="false">
      <xs:extension base="structures:AugmentationType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CaseCategoryCode" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CaseNewIndicator" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:CaseParty" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CaseShortTitleText" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:CaseTrackingID" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CaseTypeCode" />
          <xs:element minOccurs="0" maxOccurs="1" ref="j:CaseNumberText" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="nc:Metadata" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="nc:OrganizationAssociation" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="nc:PersonAssociation" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="nc:PersonOrganizationAssociation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CaseFilingType">
    <xs:annotation>
      <xs:documentation>The core information contained in an ECF 5.0 message.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="nc:DocumentType">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" ref="ecf:SendingMDELocationID" />
