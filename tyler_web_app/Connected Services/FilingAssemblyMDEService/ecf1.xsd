          <xs:element minOccurs="1" maxOccurs="1" ref="j:CaseCourt" />
          <xs:element minOccurs="0" maxOccurs="1" ref="nc:DocumentInformationCutOffDate" />
          <xs:element minOccurs="1" maxOccurs="1" ref="nc:DocumentPostDate" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CaseOfficialAugmentationType">
    <xs:complexContent mixed="false">
      <xs:extension base="structures:AugmentationType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:CaseRepresentedParty" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CourtEventActorType">
    <xs:annotation>
      <xs:documentation>The actor who performed the action as set forth in the docket entry. E.g. the person who filed the document.  Does not include the name of the court clerk composing the docket entry.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" ref="nc:RoleOfAbstract" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CourtEventAugmentationType">
    <xs:complexContent mixed="false">
      <xs:extension base="structures:AugmentationType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:ChildDocket" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:ConnectedDocument" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ecf:CourtEventActor" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CourtEventEnteredOnDocketDate" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CourtEventLocationText" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CourtEventOnBehalfOfActor" />
          <xs:element minOccurs="1" maxOccurs="1" ref="ecf:CourtEventTypeCode" />
          <xs:element minOccurs="0" maxOccurs="1" ref="ecf:CourtLocationCode" />
