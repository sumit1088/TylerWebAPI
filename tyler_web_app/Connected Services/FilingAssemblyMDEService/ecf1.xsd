<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" xmlns:clsa="http://reference.niem.gov/niem/specification/code-lists/1.0/code-lists-schema-appinfo/" xmlns:term="http://release.niem.gov/niem/localTerminology/3.0/" xmlns:biom="http://release.niem.gov/niem/domains/biometrics/4.1/" xmlns:structures="http://release.niem.gov/niem/structures/4.0/" xmlns:cbrn="http://release.niem.gov/niem/domains/cbrn/4.1/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/6.1/" xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/" xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/" xmlns:ecf="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="https://docs.oasis-open.org/legalxml-courtfiling/ns/v5.0/ecf" version="5.0" ct:conformanceTargets="http://reference.niem.gov/niem/specification/naming-and-design-rules/3.0/#ExtensionSchemaDocument http://reference.niem.gov/niem/specification/code-lists/1.0/#SchemaDocument" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="niem\domains\biometrics\4.1\biom.xsd" namespace="http://release.niem.gov/niem/domains/biometrics/4.1/" />
  <xs:import schemaLocation="niem\domains\cbrn\4.1\cbrn.xsd" namespace="http://release.niem.gov/niem/domains/cbrn/4.1/" />
  <xs:import schemaLocation="niem\domains\jxdm\6.1\jxdm.xsd" namespace="http://release.niem.gov/niem/domains/jxdm/6.1/" />
  <xs:import schemaLocation="niem\niem-core\4.0\niem-core.xsd" namespace="http://release.niem.gov/niem/niem-core/4.0/" />
