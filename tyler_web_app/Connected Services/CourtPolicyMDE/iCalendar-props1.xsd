    <xs:complexContent mixed="false">
      <xs:extension base="xcal:UtcOffsetPropertyType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TzoffsettoPropType">
    <xs:complexContent mixed="false">
      <xs:extension base="xcal:UtcOffsetPropertyType" />
    </xs:complexContent>
  </xs:complexType>
