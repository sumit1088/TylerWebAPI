<div id="myDiv">
    <div class="container white-bg" id="container">
        <div class="row">
            <div class="col-md-12">
                <section>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-md-12">
                                <h3 class="text-left  clearfix">
                                    <label class="pull-left" for="My_Uploads">Case Details</label>
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="form-wrap">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="table-responsive" style="min-height:.01%;overflow-x:hidden;">
                                    <table id="casedetails" class="table table-striped table-bordered" cellspacing="0" width="100%">                                        
                                        <tbody>
                                            <tr>
                                                <th> Case Title Text </th>
                                                <td>
                                                    @Model.CaseTitleText
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Case Category Text </th>
                                                <td>
                                                    @Model.CaseCategoryText
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Case Tracking ID </th>
                                                <td>
                                                    @Model.CaseTrackingID
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Case Docket ID </th>
                                                <td>
                                                    @Model.CaseDocketID
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Person Name </th>
                                                <td>
                                                    @Model.PersonGivenName
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Person Middle Name </th>
                                                <td>
                                                    @Model.PersonMiddleName
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Person SurName </th>
                                                <td>
                                                    @Model.PersonSurName
                                                </td>
                                            </tr>
                                            <tr>
                                                <th> Judicial Official Bar Identification </th>
                                                <td>
                                                    @Model.memberShip
                                                </td>
                                            </tr>
                                        </tbody>                                       
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>
